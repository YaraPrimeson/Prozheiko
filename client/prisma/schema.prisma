// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
  // url      = "mongodb://localhost:27017/prisma"
}

model Image {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  originalname String
  mimetype     String
  path         String
}

model Tag {
  id  String @id @default(auto()) @map("_id") @db.ObjectId
  tag String
}
